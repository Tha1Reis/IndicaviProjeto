
<form name="email-senha">
    <div class="card-cad" style="border-radius: 1rem;">
    <div class="form-row" id="caixa-cad1">
        <div class="form-group col-md-6">
            <input type="text" id="name-cad" class="form-control" placeholder="Nome Completo" required>
        </div>
        <br/>
        <div class="form-group col-md-6">
            <input type="text" id="cpf-cad" class="form-control" name="cpf" \
			pattern="\d{3}\.\d{3}\.\d{3}-\d{2}" \
			title="Digite um CPF no formato: xxx.xxx.xxx-xx"
            placeholder="CPF"
            required>
        </div>
        <br/>
        <div class="form-group col-md-6">
            <input type="email" name="email"class="form-control" id="email" placeholder="E-mail" \
             required>
        </div>
        <br/>
        <div class="form-group col-md-6">
            <input type="password" name="password" class="form-control" id="password" placeholder="Senha" required>
        </div>
        <br/>
        <div class="form-group col-md-4">
            <select id="selecione-cad" class="form-control" name="selecione" required>
                <option selected hidden >Selecione</option>
                <option value="AC">Prestador de Servi√ßo</option>
                <option value="AL">Cliente</option>
            </select>
        </div>
    </div>
    <button type="submit" class="btn btn-primary" id="bot-cad" name="signup">Cadastrar-se</button>
     <a class="btn btn-secondary" asp-controller="Login" asp-action="Index" role="button " id="caixa-cvol"><</a>
    </div>
</form>

<script src="https://www.gstatic.com/firebasejs/7.0.0/firebase-app.js" ></script>
<script src="https://www.gstatic.com/firebasejs/7.0.0/firebase-auth.js" ></script>

<script src="js/init.js"></script>
<script src="js/email-senha.js"></script>